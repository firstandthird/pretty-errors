{
  "name": "@firstandthird/pretty-errors",
  "version": "2.0.0",
  "description": "Domodule Library for adding nice errors on web sites.",
  "main": "index.js",
  "module": "dist/pretty-errors.js",
  "homepage": "https://github.com/firstandthird/pretty-errors",
  "author": "First+Third",
  "license": "MIT License",
  "repository": {
    "type": "git",
    "url": "https://github.com/firstandthird/pretty-errors"
  },
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "pretest": "scriptkit build",
    "test": "cat test/pretty-errors.test.dist.bundle.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublishOnly": "scriptkit"
  },
  "devDependencies": {
    "eslint-config-firstandthird": "^4.4.0",
    "eslint-plugin-compat": "^3.2.0",
    "eslint-plugin-import": "^2.18.0",
    "phantomjs-prebuilt": "^2.1.16",
    "scriptkit": "^3.0.0",
    "tap-spec": "^5.0.0",
    "tape-rollup": "^4.6.4",
    "tape-run": "^6.0.0"
  },
  "dependencies": {
    "domassist": "^2.2.0",
    "domodule": "^6.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird",
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": 2
    }
  },
  "scriptkit": {
    "files": {
      "dist/pretty-errors.js": "index.js",
      "test/pretty-errors.test.dist.js": "test/pretty-errors.test.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  }
}
